{
  "label": "KG Extractor",
  "description": "Extracts entities and relationships from text using Gemini and creates them in the knowledge graph",
  "endpoint": "https://kg-extractor.nick-chimicles-professional.workers.dev",
  "actions_required": ["entity:view", "entity:create", "entity:update", "entity:delete"],
  "accepts": {
    "types": ["text_chunk", "document", "*"],
    "cardinality": "one"
  },
  "produces": {
    "types": ["person", "creature", "ship", "place", "group", "object", "concept", "organization", "event", "entity"],
    "cardinality": "many"
  }
}

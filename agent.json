{
  "label": "KG Extractor v2",
  "description": "Extracts entities and relationships from text using Gemini (Durable Object based for long-running operations)",
  "endpoint": "https://kg-extractor-v2.nick-chimicles-professional.workers.dev",
  "actions_required": ["entity:view", "entity:create", "entity:update", "entity:delete"],
  "accepts": {
    "types": ["text_chunk", "document", "*"],
    "cardinality": "one"
  },
  "produces": {
    "types": ["person", "creature", "ship", "place", "group", "object", "concept", "organization", "event", "entity"],
    "cardinality": "many"
  }
}
